{
	"name": "opsiweb",
	"dockerComposeFile": "../docker/docker-compose.yml",
	"service": "nuxt",
	"workspaceFolder": "/workspace/opsiweb",
	"forwardPorts": [8888],
	// "postStartCommand": "npm install && PORT=8888 bash",
	"postStartCommand": "npm run clean && npm install && chown -f -R 1000:1000 /root/.npm && chown -f -R 1000:1000 /root/.npm/* && PORT=8888 npm run dev",
	// "remoteUser": "1000",
	"extensions": [
		"octref.vetur",
		"dbaeumer.vscode-eslint",
		"gruntfuggly.todo-tree",
		"fooxly.workspace",
		"esbenp.prettier-vscode",
		"lokalise.i18n-ally"
	]
}