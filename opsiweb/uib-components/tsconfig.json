{
	"compilerOptions": {
		"experimentalDecorators": true,
		"importHelpers": true,
		"target": "ES6",
		"module": "ESNext",
		"noImplicitAny": false,
		"moduleResolution": "Node",
		"lib": [
			"ESNext",
			"ESNext.AsyncIterable",
			"DOM"
		],
		"esModuleInterop": true,
		"allowJs": true,
		"sourceMap": true,
		"strict": true,
		"noEmit": true,
		"baseUrl": ".",
		"paths": {
		"~/*": [
			"./*"
		],
		"@/*": [
			"./*"
		]
		},
		"types": [
			"@types/node",
			"@types/jest",
			"@nuxt/types",
			"@nuxtjs/i18n",
			"@nuxtjs/axios",
			"bootstrap-vue",
			"webpack-env"
		]
	},
	"vueCompilerOptions": {
		"experimentalDisableTemplateSupport": true,
		"experimentalCompatMode": 2,
		"experimentalTemplateCompilerOptions": {
			"compatConfig": { "MODE": 2 }
		}
	},
	"exclude": [
		"node_modules"
	]
  }
