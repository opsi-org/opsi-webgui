{
  "name": "webgui",
  "version": "4.3.33",
  "private": true,
  "repository": "https://gitlab.uib.gmbh/uib/opsiweb-ui",
  "license": "MIT",
  "keywords": [
    "opsi",
    "webgui",
    "uib"
  ],
  "description": "uib opsiweb",
  "scripts": {
    "//project-handling": "",
    "createpackagejson": "echo 'build'",
    "build": "npm i",
    "dev": "PORT=8888 NODE_ENV=development NODE_OPTIONS=--inspect nuxt . --hostname 0.0.0.0",
    "clean": "rm -rf node_modules uib-components/node_modules; rm -f package-lock.json uib-components/package-lock.json",
    "clean:all": "rm -rf node_modules uib-components/node_modules ./node_modules .nuxt ./.nuxt .nuxt-storybook ./.nuxt-storybook test-results ./test-results uib-components/test-results ./dist ; rm -f package.json package-lock.json package-merged.json uib-components/package-lock.json test-result-pw-reporter-* ;",
    "buildclean": "npm run clean && npm run build",
    "buildclean-lib": "cd uib-components && npm run buildclean",
    "prod": "nuxt generate ./ && npm run start",
    "start": "nuxt start -p 4445 ./ ",
    "generate": "NODE_OPTIONS=--openssl-legacy-provider nuxt generate ./ ",
    "generate-nossl": "nuxt generate ./ ",
    "//storybook": "commands for storybook",
    "story": "rm -rf uib-components/node_modules; PORT=3003 NODE_ENV=development NODE_OPTIONS=--inspect nuxt-storybook . --ci --quiet --port 3003 -s ./uib-components/static",
    "story-components": "PORT=3003 NODE_ENV=development NODE_OPTIONS=--inspect nuxt-storybook  uib-components/ --ci --quiet --port 3003 -s ./uib-components/static",
    "story2": "PORT=3003 NODE_ENV=development NODE_OPTIONS=--inspect storybook --port 3003 -s ./uib-components/static",
    "//lint": "commands for linting",
    "lint-script": "eslint --quiet --ext \"ts,js,vue,json\" --ignore-path .eslintignore ",
    "lint-ui": "eslint --quiet --ext \"ts,js,vue,json\" --ignore-path .eslintignore ./",
    "lint-lib": "eslint --quiet --ext \"ts,js,vue,json\" --ignore-path .eslintignore  uib-components/",
    "lint:all": "npm run lint-ui && npm run lint-lib",
    "lint": "npm run lint-ui",
    "//npm packages": "commands for handling package dependencies ",
    "dependencies-list-upgradeable": "ncu",
    "dependencies-update-package-json": "ncu -u",
    "//test": "commands for testing unit/playwright",
    "test": "jest && playwright test test/usecase test/accessibility test/component",
    "test:unit:current": "jest --config .config/.jest.config.js  ",
    "test:unit": "jest --config .config/.jest.config.js  ./",
    "test:unit-lib": "jest --config uib-components/.config/.jest.config.js  uib-components/",
    "test:unit:components": "jest --config uib-components/.config/.jest.config.js uib-components/",
    "test:component": "cd uib-components && playwright test -c .utils/playwright/config/playwright-component.config.js",
    "test:usecase": "cd uib-components && playwright test -c .utils/playwright/config/playwright-usecase.config.js",
    "test:accessibility": "cd uib-components && playwright test -c .utils/playwright/config/playwright-accessibility.config.js",
    "test:delete-empty-results": "find uib-components/test-results -empty -delete; find ./test-results -empty -delete",
    "test:all:delete-empty-results": "find test-results -empty -delete",
    "test:all:unit": "jest --config .config/.jest.config.js  ./ ",
    "test:all:usecase": "playwright test -c .config/playwright-all-usecases.config.js --project=Chromium-Desktop",
    "test:all:accessibility": "playwright test -c .config/playwright-all-accessibility.config.js --project=Chromium-Desktop",
    "//gittest:all:accessibility": "playwright test -c .config/playwright-all-accessibility.config.js --project=Chromium-Desktop --shard=$CI_NODE_INDEX/$CI_NODE_TOTAL",
    "test:components:precommit": "playwright test -c .config/playwright-all-components.config.js --project=Chromium-Desktop",
    "test:all:components": "playwright test -c .config/playwright-all-components.config.js --project=Chromium-Desktop",
    "test:all:components-devices": "playwright test -c .config/playwright-all-components.config.js ",
    "test:all:components:update": "playwright test -c .config/playwright-all-components.config.js --update-snapshots --project=Chromium-Desktop",
    "test:all:components-devices:update": "playwright test -c .config/playwright-all-components.config.js --update-snapshots ",
    "vscode:test:all:components": "playwright"
  },
  "eslintConfig": {
    "extends": [
      ".config/.eslintrc.js"
    ]
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0-beta2",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/i18n": "^7.3.x",
    "@riophae/vue-treeselect": "^0.4.0",
    "babel-runtime": "^6.26.0",
    "bootstrap": "^5.x.x",
    "bootstrap-vue": "^2.x.x",
    "core-js": "^3.x.x",
    "js-cookie": "^3.x.x",
    "nuxt": "^2.15.8",
    "nuxt-mq": "^2.0.2",
    "nuxt-property-decorator": "^2.9.1",
    "vue-markdown": "^2.2.4",
    "vuex-persist": "^3.1.3",
    "xterm": "^5.2.x",
    "xterm-addon-fit": "0.7.0",
    "xterm-addon-search": "0.12.0",
    "xterm-addon-web-links": "0.8.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.7.3",
    "@babel/eslint-parser": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@nuxt/types": "^2.x.x",
    "@nuxt/typescript-build": "^2.x.x",
    "@nuxtjs/eslint-config-typescript": "^9.x.x",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/storybook": "^4.3.0",
    "@playwright/test": "^1.39.0",
    "@types/jest": "^26.x.x",
    "@types/js-cookie": "^3.x.x",
    "@types/webpack-env": "^1.x.x",
    "@vue/test-utils": "^1.1.3",
    "axios-mock-adapter": "^1.x.x",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.x.x",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.x.x",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsonc": "^2.x.x",
    "eslint-plugin-nuxt": "^3.x.x",
    "eslint-plugin-vue": "^8.x.x",
    "eslint-plugin-vuejs-accessibility": "^1.2.0",
    "jest": "^26.x.x",
    "jest-silent-reporter": "^0.5.0",
    "npm-check-updates": "^16.x.x",
    "ts-jest": "^26.x.x",
    "ts-node": "^10.x.x",
    "tslint": "^6.1.3",
    "typescript": "^4.x.x",
    "vue-jest": "^4.0.1",
    "vuex-module-decorators": "^0.x.x"
  }
}