{
  "name": "webgui",
  "version": "0.0.7",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules uib-components/node_modules; rm -f package-lock.json uib-components/package-lock.json",
    "build": "dry i --dry-save-package-json-to package-merged.json --dry-keep-package-json",
    "dev": "PORT=8888 NODE_ENV=development NODE_OPTIONS=--inspect nuxt ui/",
    "cleanbuild": "npm run clean && npm run build",
    "cleanbuilddev": "npm run clean && npm run build && npm run dev",
    "story": "PORT=3003 NODE_ENV=development NODE_OPTIONS=--inspect nuxt ui/ storybook --ci --quiet --port 3003 -s ./ui/static",
    "lint": "dry run lint:ui",
    "lint2": "eslint .",
    "test": "jest && playwright test test/e2e test/integration",
    "test:unit": "jest --config ui/jest.config.js  ui/",
    "test:integration": "playwright test -c uib-components/.utils/playwright/config/playwright-integration.config.js",
    "test:e2e": "playwright test -c uib-components/.utils/playwright/config/playwright-e2e.config.js",
    "test:integration:delete-empty-results": "find test-results -empty -delete",
    "buildclean": "dry run clean && dry run build #&& dry run dev",
    "buildclean-lib": "cd uib-components && dry run buildclean",
    "story-components": "PORT=3003 NODE_ENV=development NODE_OPTIONS=--inspect nuxt uib-components/ storybook --ci --quiet --port 3003 -s ./ui/static",
    "story2": "PORT=3003 NODE_ENV=development NODE_OPTIONS=--inspect storybook --port 3003 -s ./ui/static",
    "prod": "nuxt ui/ generate && dry run start",
    "start": "nuxt ui/ start",
    "generate": "nuxt ui/ generate",
    "lint:ui": "eslint --quiet --ext \".ts,.js,.vue\" --ignore-path ui/.gitignore  ui/",
    "lint:components": "eslint --quiet --ext \".ts,.js,.vue\" --ignore-path uib-components/.gitignore  uib-components/",
    "lint:all": "dry run lint:ui && dry run lint:components",
    "test:unit:components": "jest --config uib-components/jest.config.js uib-components/",
    "test:playwright": "# TODO: command to test both",
    "all": "../.devcontainer/npm_all.sh"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/i18n": "^7.0.3",
    "@riophae/vue-treeselect": "^0.4.0",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.15.1",
    "js-cookie": "^2.2.1",
    "nuxt": "^2.15.8",
    "nuxt-mq": "^2.0.2",
    "nuxt-property-decorator": "^2.9.1",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@babel/preset-typescript": "^7.15.0",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/storybook": "^4.1.1",
    "@playwright/test": "^1.17.1",
    "@types/jest": "^27.0.2",
    "@types/js-cookie": "^2.2.6",
    "@types/webpack-env": "^1.16.2",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.29.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "jest": "^26.6.3",
    "jest-silent-reporter": "^0.5.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4",
    "vue-jest": "^4.0.1",
    "vuex-module-decorators": "^1.0.1"
  },
  "repository": "https://gitlab.uib.gmbh/uib/opsiweb-ui-components",
  "license": "MIT",
  "keywords": [
    "opsi",
    "webgui",
    "uib"
  ],
  "description": "uib opsiweb"
}
