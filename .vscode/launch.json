{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "webgui",
			"runtimeExecutable": "npm",
			"runtimeArgs": [
				"run-script",
				"dev"
			],
			"outputCapture": "std",
			"request": "launch",
			"type": "node",
			"skipFiles": [
				"<node_internals>/**"
			],
			"auto": true,
			"cwd": "${workspaceFolder}/opsiweb"
		},
		{
			"name": "storybook",
			"runtimeExecutable": "npm",
			"runtimeArgs": [
				"run-script",
				"story"
			],
			"outputCapture": "std",
			"request": "launch",
			"type": "node",
			"skipFiles": [
				"<node_internals>/**"
			],
			"auto": true,
            "cwd": "${workspaceFolder}/opsiweb"
		}
    ]
}
// echo "start application at port 8888"
