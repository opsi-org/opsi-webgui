{
	"compilerOptions": {
	  "experimentalDecorators": true,
	  "importHelpers": true,
	  "target": "ES6",
	  // "target": "ES2018",
	  "module": "ESNext",
	  "moduleResolution": "Node",
	  "lib": [
		"ESNext",
		"ESNext.AsyncIterable",
		"DOM"
	  ],
	  "esModuleInterop": true,
	  "allowJs": true,
	  "sourceMap": true,
	  "strict": true,
	  "noEmit": true,
	  "baseUrl": ".",
	  "paths": {
		"~/*": [
		  "./*"
		],
		"@/*": [
		  "./*"
		]
	  },
	  "types": [
		"@types/node",
		"@types/jest",
		"@nuxt/types",
		"@nuxtjs/i18n",
		"@nuxtjs/axios",
		"bootstrap-vue",
		"webpack-env"
	  ]
	},
	"exclude": [
	  "node_modules"
	],
  }
