{
  "name": "uib-components",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "libclean": "rm -rf node_modules ; rm -f package-lock.json",
    "libbuild": "npm i",
    "libdev": "PORT=8899 NODE_ENV=development NODE_OPTIONS=--inspect nuxt ",
    "libcleanbuild": "npm run clean && npm run build",
    "libcleanbuilddev": "npm run clean && npm run build && npm run dev",
    "libstory": "PORT=3003 NODE_ENV=development NODE_OPTIONS=--inspect nuxt storybook --ci --quiet --port 3003 -s ./static",
    "liblint": "eslint --quiet --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "liblint2": "eslint .",
    "libtest": "jest && playwright test test/e2e test/integration",
    "libtest:unit": "jest --config .config/.jest.config.js .",
    "libtest:integration": "playwright test -c .utils/playwright/config/playwright-integration.config.js  --project=Chromium-Desktop",
    "libtest:e2e": "playwright test -c .utils/playwright/config/playwright-e2e.config.js --project=Chromium-Desktop",
    "libtest:delete-empty-results": "find test-results -empty -delete"
  },
  "eslintConfig": {
    "extends": ".config/.eslintrc.js"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/i18n": "^7.0.3",
    "@riophae/vue-treeselect": "^0.4.0",
    "bootstrap": "^5.x.x",
    "bootstrap-vue": "^2.x.x",
    "core-js": "^3.15.1",
    "js-cookie": "^3.x.x",
    "nuxt": "^2.15.8",
    "nuxt-mq": "^2.0.2",
    "nuxt-property-decorator": "^2.9.1",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@babel/preset-typescript": "^7.15.0",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^9.x.x",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/storybook": "^4.3.0",
    "@playwright/test": "^1.17.2",
    "@types/jest": "^27.0.2",
    "@types/js-cookie": "^3.x.x",
    "@types/webpack-env": "^1.16.2",
    "@vue/runtime-dom": "latest",
    "@vue/test-utils": "^1.1.3",
    "axios-mock-adapter": "^1.20.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^8.x.x",
    "eslint-plugin-nuxt": "^3.x.x",
    "eslint-plugin-vue": "^8.x.x",
    "jest": "^26.6.3",
    "jest-silent-reporter": "^0.5.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "typescript": "^4.6.0",
    "vue-jest": "^4.0.1",
    "vuex-module-decorators": "^2.x.x"
  }
}
